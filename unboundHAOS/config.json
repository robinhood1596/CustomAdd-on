{
  "name": "Unbound DNS Resolver",
  "version": "0.1.0",
  "slug": "unbound",
  "description": "A secure, validating, recursive DNS resolver for Home Assistant.",
  "url": "https://github.com/robinhood1596/CustomAdd-on/tree/main/unboundHAOS",
  "startup": "services",
  "init": false,
  "host_network": true,
  "ports": {
    "54/tcp": null,
    "54/udp": null
  },
  "ports_description": {
    "54/tcp": "Unbound DNS TCP port (for zone transfers, etc.)",
    "54/udp": "Unbound DNS UDP port (main query port)"
  },
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7",
    "i386"
  ],
  "options": {
    "listen_port": 54,
    "verbosity": 1,
    "access_control_ips": ["192.168.178.0/24", "172.16.0.0/12", "10.0.0.0/8"]
  },
  "schema": {
    "listen_port": "int(1,65535)",
    "verbosity": "int(0,5)",
    "access_control_ips": ["str"]
  },
  "image": "klutchell/unbound:{arch}"
}
